{
	"name": "MeetChrome",
	"version": "1.0",
	"manifest_version": 2,
	"description": "A simple, handy Meetup client for Chrome.",
	"icons": { 
		"16": "icon16.png",
		"48": "icon48.png",
		"128": "icon128.png"
	},
	"browser_action": {
		"default_icon": "icon19.png",
		"default_popup": "popup.html"
	},
		"permissions": [
		"http://api.meetup.com/"
	]
}
